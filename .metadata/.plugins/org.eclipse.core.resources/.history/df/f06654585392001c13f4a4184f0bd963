package study2;

//조상 클래스
public class Car {
	private String color;
	private int door;
	
	
	public Car(String color, int door) {
		super();
		this.color = color;
		this.door = door;
	}

	public void drive() {
		System.out.println("차가 달립니다.");
	}
	
	public void stop() {
		System.out.println("차가 멈춥니다.");
	}
	
}
