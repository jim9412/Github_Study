package study;

public class MathEx {

	public static void main(String[] args) {

		
		int v1 = Math.abs(-5);
		double v2 = Math.abs(-7.88);
		System.out.println(v1);
		System.out.println(v2);
		
		double v3 = Math.ceil(5.1);
		System.out.println(v3);
		
		double v4 = Math.rint(5.9);
		System.out.println(v4);
	}

}
