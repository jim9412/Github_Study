package study;

public class Student{
	int age;
	String name;
	public Student(int age, String name) {
		this.age =age;
		this.name = name;
	}
	
	@Override
	public boolean equals(Object obj) {
		if(obj instanceof Student) {
			Student student = (Student)obj;
		
			if( (this.age == student.age) && (this.name.equals(student.name))) {
			return true;
			}
		}	
		return false;
	}
	
}